  <circle cx="50" cy="50" r="80" fill="url(#audit-gradient1)" opacity="0.1"/>
  <circle cx="350" cy="150" r="100" fill="url(#audit-gradient2)" opacity="0.1"/>
  
  <!-- Gavel/Hammer -->
  <rect x="180" y="100" width="120" height="20" rx="5" fill="#1E3A8A"/>
  <rect x="220" y="60" width="40" height="40" rx="5" fill="#1E3A8A"/>
