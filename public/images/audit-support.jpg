  <path d="M120 90 L130 95 V115 C130 120 125 125 120 130 C115 125 110 120 110 115 V95 Z" fill="#10B981" opacity="0.8"/>
  <path d="M115 115 L120 120 L125 110" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
  
  <!-- Document Stack -->
  <rect x="280" y="80" width="60" height="80" rx="3" fill="#1E293B" stroke="#334155" stroke-width="1"/>
  <rect x="285" y="75" width="60" height="80" rx="3" fill="#1E293B" stroke="#334155" stroke-width="1"/>
  <rect x="290" y="70" width="60" height="80" rx="3" fill="white" stroke="#334155" stroke-width="1"/>
  
