<svg width="400" height="200" viewBox="0 0 400 200" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="200" fill="#0F172A"/>
  
  <!-- Decorative Elements -->
  <circle cx="50" cy="50" r="80" fill="url(#plan-gradient1)" opacity="0.1"/>
  <circle cx="350" cy="150" r="100" fill="url(#plan-gradient2)" opacity="0.1"/>
  
  <!-- Graph Background -->
  <rect x="100" y="40" width="200" height="120" rx="5" fill="#1E293B" stroke="#334155" stroke-width="2"/>
  
  <!-- Graph Grid -->
  <line x1="100" y1="160" x2="300" y2="160" stroke="#334155" stroke-width="1"/>
  <line x1="100" y1="130" x2="300" y2="130" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="100" y1="100" x2="300" y2="100" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="100" y1="70" x2="300" y2="70" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  
  <line x1="130" y1="40" x2="130" y2="160" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="160" y1="40" x2="160" y2="160" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="190" y1="40" x2="190" y2="160" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="220" y1="40" x2="220" y2="160" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="250" y1="40" x2="250" y2="160" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  <line x1="280" y1="40" x2="280" y2="160" stroke="#334155" stroke-width="1" stroke-dasharray="4 4"/>
  
  <!-- Bar Chart -->
  <rect x="120" y="120" width="20" height="40" fill="#1E3A8A" opacity="0.7"/>
  <rect x="150" y="100" width="20" height="60" fill="#1E3A8A" opacity="0.8"/>
  <rect x="180" y="90" width="20" height="70" fill="#10B981" opacity="0.7"/>
  <rect x="210" y="70" width="20" height="90" fill="#10B981" opacity="0.8"/>
  <rect x="240" y="80" width="20" height="80" fill="#10B981" opacity="0.9"/>
  <rect x="270" y="60" width="20" height="100" fill="#10B981" opacity="1"/>
  
  <!-- Line Chart -->
  <path d="M130 110 L160 90 L190 100 L220 70 L250 80 L280 60" stroke="#10B981" stroke-width="3" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
  
  <!-- Data Points -->
  <circle cx="130" cy="110" r="4" fill="#10B981"/>
  <circle cx="160" cy="90" r="4" fill="#10B981"/>
  <circle cx="190" cy="100" r="4" fill="#10B981"/>
  <circle cx="220" cy="70" r="4" fill="#10B981"/>
  <circle cx="250" cy="80" r="4" fill="#10B981"/>
  <circle cx="280" cy="60" r="4" fill="#10B981"/>
  
  <!-- Calendar Icon -->
  <rect x="320" y="50" width="30" height="30" rx="3" fill="#1E3A8A" opacity="0.8"/>
  <rect x="325" y="55" width="20" height="20" rx="2" fill="#0F172A"/>
  <path d="M330 60 H340 M330 65 H340 M330 70 H340" stroke="#10B981" stroke-width="1.5" stroke-linecap="round"/>
  
  <!-- Magnifying Glass -->
  <circle cx="70" cy="120" r="15" stroke="#1E3A8A" stroke-width="3" fill="none"/>
  <line x1="80" y1="130" x2="90" y2="140" stroke="#1E3A8A" stroke-width="3" stroke-linecap="round"/>
  
  <!-- Gradients -->
  <defs>
    <linearGradient id="plan-gradient1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1E3A8A" />
      <stop offset="100%" stop-color="#10B981" />
    </linearGradient>
    <linearGradient id="plan-gradient2" x1="100%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" stop-color="#1E3A8A" />
      <stop offset="100%" stop-color="#10B981" />
    </linearGradient>
  </defs>
</svg>